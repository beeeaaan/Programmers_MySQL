SELECT CAR_ID, ROUND(AVG(DATEDIFF(END_DATE, START_DATE) + 1), 1) AS AVERAGE_DURATION  // 평균 대여 기간 구하기
FROM CAR_RENTAL_COMPANY_RENTAL_HISTORY
GROUP BY CAR_ID
HAVING AVERAGE_DURATION >= 7  // 평균 대여 기간 7일 이상
ORDER BY AVERAGE_DURATION DESC, CAR_ID DESC;

// ROUND(숫자, 반올림할 자릿수) : 숫자를 반올림할 자릿수 +1 자릿수에서 반올림
